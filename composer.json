{
  "name": "wearegenuine/gnorm-twig",
  "type": "library",
  "description": "Supplemental library for compiling Gnorm Twig files.",
  "license": "MIT",
  "authors": [
    {
      "name": "Les Peabody",
      "email": "lpeabody@wearegenuine.com"
    },
    {
        "name": "Patrick Connors",
        "email": "pconnors@wearegenuine.com"
    },
    {
        "name": "Ryan Brooks",
        "email": "rbrooks@wearegenuine.com"
    },
    {
        "name": "Chase Barrett",
        "email": "cbarrett@wearegenuine.com"
    }
  ],
  "require": {
      "php": "^7.4 || ^8.0",
      "ext-json": "*",
      "twig/twig": "^2.15.3 || ^3.5.0"
  },
  "autoload": {
      "psr-4": {
          "Gnorm\\": "src",
          "Tests\\": "tests"
      }
  },
  "bin": [
    "bin/compile.php"
  ],
  "config": {
      "sort-packages": true
  },
  "require-dev": {
    "php-mock/php-mock": "^2.5",
    "php-mock/php-mock-phpunit": "^2.10",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpstan/phpstan": "^1.9",
    "phpunit/phpunit": "^11.3",
    "squizlabs/php_codesniffer": "^3.6"
  }
}
